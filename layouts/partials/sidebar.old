
<ul id="slide-out" class="sidenav sidenav-fixed">
    <li class="logo"><a id="logo-container" href="/" class="brand-logo">BMEG</a></li>
    {{ $page := . }}
    {{ range $i, $menu := .Site.Menus }}
      {{ range $menu }}
      <li>
        {{ if .HasChildren }}
        <span class="intermediate"><a href="{{ .URL }}"
           class="sidebar-nav-item {{ if $page.IsMenuCurrent "main" . }} active{{end}}"
        >{{ .Pre }}{{ .Name }}</a></span></li>
        {{ range .Children }}
        <li>
          <ul class="collapsible-header">
            <li>
              <a href="{{ .URL }}"
                 class="sidebar-nav-item {{ if $page.IsMenuCurrent "main" . }} active{{end}}"
              >{{ .Name }}</a></li>
          </ul>
        </li>
        {{ end }}
        {{ else }}
        <a href="{{ .URL }}"
           class="sidebar-nav-item {{ if $page.IsMenuCurrent "main" . }} active{{end}}"
        >{{ .Pre }}{{ .Name }}</a></li>
        {{ end }}
      {{ end }}
    {{end}}
</ul>
